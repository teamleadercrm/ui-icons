{
  "name": "@teamleader/ui-icons",
  "description": "Teamleader UI icons",
  "version": "0.2.11",
  "author": "Teamleader <development@teamleader.eu> (https://www.teamleader.eu)",
  "bugs": {
    "url": "https://github.com/teamleadercrm/ui-icons/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@balajmarius/svg2jsx": "^1.0.8",
    "bluebird": "^3.5.1",
    "cheerio": "^1.0.0-rc.2",
    "cli-color": "^1.2.0",
    "fs-extra": "^4.0.2",
    "glob": "^7.1.2",
    "lodash.camelcase": "^4.3.0",
    "lodash.findkey": "^4.6.0",
    "lodash.upperfirst": "^4.3.1",
    "prettier": "^1.16.4"
  },
  "engines": {
    "node": "^8.0.0",
    "npm": "^5.0.0"
  },
  "homepage": "https://github.com/teamleadercrm/ui-icons#readme",
  "jsnext:main": "es/index.js",
  "keywords": [
    "icons",
    "teamleader",
    "ui"
  ],
  "license": "MIT",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teamleadercrm/ui-icons.git"
  },
  "scripts": {
    "build": "npm run build:es; npm run build:cjs",
    "build:cjs": "NODE_ENV=cjs babel lib --out-dir cjs",
    "build:es": "NODE_ENV=es babel lib --out-dir es",
    "lint:src": "prettier \"{bin,src}/*.js\"",
    "lint:src:fix": "prettier --write \"{bin,src}/*.js\"",
    "lint:lib": "prettier \"lib/*.js\"",
    "lint:lib:fix": "prettier --write \"lib/*.js\"",
    "lint": "npm run lint:src; npm run lint:lib",
    "lint:fix": "npm run lint:src:fix; npm run lint:lib:fix",
    "prebuild": "node ./bin/create && npm run lint:lib:fix",
    "prepare": "npm run build"
  },
}
